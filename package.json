  "scripts": {
    "start": "ng serve",
    "test": "ng test",
    "build": "webpack --config webpack.config.js",
    "build:prod": "webpack --config webpack.config.js --env.MODE=prod",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "build-conf:aot:prod": "npm run clean:dist && npm run clean:aot && webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build-conf:aot": "npm run build-conf:aot:prod",
    "build-conf:dev": "npm run clean:dist && webpack --config config/webpack.dev.js --progress --profile",
    "build-conf:docker": "npm run build-conf:prod && docker build -t angular2-webpack-start:latest .",
    "build-conf:prod": "npm run clean:dist && webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build-conf": "npm run build-conf:dev",
    "ci:aot": "npm run lint && npm run test && npm run build-conf:aot && npm run e2e",
    "ci:jit": "npm run lint && npm run test && npm run build-conf:prod && npm run e2e",
    "ci:nobuild": "npm run lint && npm test && npm run e2e",
    "ci:testall": "npm run lint && npm run test && npm run build-conf:prod && npm run e2e && npm run build-conf:aot && npm run e2e",
    "ci:travis": "npm run lint && npm run test && npm run build-conf:dev && npm run e2e:travis && npm run build-conf:prod && npm run e2e:travis && npm run build-conf:aot && npm run e2e:travis",
    "ci": "npm run ci:testall",
    "clean:dll": "npm run rimraf -- dll",
    "clean:aot": "npm run rimraf -- compiled",
    "clean:dist": "npm run rimraf -- dist",
    "clean:install": "npm set progress=false && npm install",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist compiled dll",
    "docker": "docker",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "e2e:live": "npm-run-all -p -r server:prod:ci protractor:live",
    "e2e:travis": "npm-run-all -p -r server:prod:ci protractor:delay",
    "e2e:prod": "npm-run-all -p -r server:prod:ci protractor",
    "github-deploy-conf:dev": "webpack --config config/webpack.github-deploy.js --progress --profile --env.githubDev",
    "github-deploy-conf:prod": "webpack --config config/webpack.github-deploy.js --progress --profile --env.githubProd",
    "github-deploy": "npm run github-deploy-conf:dev",
    "postinstall": "npm run webdriver:update",
    "postversion": "git push && git push --tags",
    "preclean:install": "npm run clean",
    "preversion": "npm test",
    "protractor": "protractor",
    "protractor:delay": "sleep 3 && npm run protractor",
    "protractor:live": "protractor --elementExplorer",
    "rimraf": "rimraf",
    "server:prod:ci": "http-server dist -p 3000 -c-1 --cors",
    "server-conf:dev:hmr": "npm run server-conf:dev -- --inline --hot",
    "server-conf:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base src/",
    "server-conf:prod": "http-server dist -c-1 --cors",
    "server-conf": "npm run server-conf:dev",
    "start-conf:hmr": "npm run server-conf:dev:hmr",
    "test:dev": "npm run lint && karma start",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "version": "npm run build",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch": "npm run watch:dev",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "webdriver-manager update",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack"
  },
